{
  "title": "Yubi enabled only when CAPS lock ON",
  "rules": [
    {
      "description": "CAPS key: toggle capskey_mode on/off",
      "manipulators": [
        {
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "mandatory": [],
              "optional": [
                "any"
              ]
            }
          },
          "type": "basic",
          "to": {
            "set_variable": {
              "name": "capskey_mode",
              "value": 1
            }
          },
          "to_after_key_up": {
            "set_variable": {
              "name": "capskey_mode",
              "value": 0
            }
          }
        }
      ]
    },
    {
